import pyqrcode 
from pyqrcode import QRCode 
  
# String which represent the QR code 
link = "https://www.youtube.com/channel/UCV4nigChEwSNyKYu2Gqxu5w"
  
# Generate QR code 
GenerateQr = pyqrcode.create(link) 
  
# Create and save the png file naming "myqr.png" 
GenerateQr.svg("myyoutube.svg", scale = 8) 